<div class="container">
  <div class="text-center well-lg background-white margin-top-50 drop-shadow">

<h1>Forgot password</h1>


    <%= form_for(:password_reset, url: password_resets_path) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.submit "Submit" %>
    <% end %>
  </div>
</div>
