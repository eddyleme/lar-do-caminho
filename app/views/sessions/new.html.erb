<h2>Log in</h2>
<% if flash.now[:notice] %>
<p class="error">
<%= flash.now[:notice] %>
</p>
<% end %>
<%= form_with scope: :session, url: sessions_path do |form| %>
<p>
<%= label_tag :email %>
<%= text_field_tag :email %>
</p> <p>
<%= label_tag :password %>
<%= password_field_tag :password %> </p>
<p>
<%= submit_tag 'Log in' %>
</p>
<% end %>